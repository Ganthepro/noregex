    "@types/react": "^18",
    "@types/react-dom": "^18",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.12",
    "husky": "^9.0.11",
    "prettier": "^3.2.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build"
    }
